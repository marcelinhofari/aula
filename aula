#include <stdio.h>

 struct jogadores 
{int num;
float peso;
float alt;
char i_nome[50];
};
    struct jogadores jg;
int main()
{
  
  printf("\nKG do 1° Jogador: ");
  scanf("%f", &jg.peso);
  printf("Altura do 1°\n");
  scanf("%f", &jg.alt);
  printf("Inicial do Jogador\n");
  scanf("%s", jg.i_nome);

    return 0;
}
